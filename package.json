{
  "name": "prismic-javascript",
  "description": "JavaScript development kit for prismic.io",
  "license": "Apache-2.0",
  "url": "https://github.com/prismicio/prismic-javascript",
  "keywords": [
    "prismic",
    "prismic.io",
    "cms",
    "content",
    "api"
  ],
  "version": "2.1.1",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "rollup": "^0.65.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^2.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismicio/prismic-javascript.git"
  },
  "main": "dist/prismic-javascript.js",
  "module": "dist/prismic-javascript.mjs",
  "browser": {
    "./dist/prismic-javascript.js": "dist/prismic-javascript.browser.js",
    "./dist/prismic-javascript.mjs": "dist/prismic-javascript.browser.mjs"
  },
  "types": "./d.ts/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "check": "tsc --pretty --noEmit --watch",
    "test": "mocha",
    "prepare": "npm run build; npm run docs; npm run declarations",
    "docs": "typedoc --mode file --out ./docs ./src/index.ts",
    "declarations": "tsc --emitDeclarationOnly --declaration --declarationDir ./d.ts",
    "lint": "tslint src/**/*.ts -e src/lru.ts -e src/Cookies.ts"
  },
  "dependencies": {
    "@types/node-fetch": "^2.1.2",
    "node-fetch": "^2.2.0"
  }
}
